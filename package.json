{
	"name": "deeplink",
	"displayName": "Mobile Dev Tools (Android / iOS)",
	"description": "Open deeplinks in emulators, push notifications, screen mirroring, WIFI debugging...",
	"version": "0.0.11",
	"publisher": "emanuel-braz",
	"repository": {
		"type": "git",
		"url": "https://github.com/emanuel-braz/deeplink"
	},
	"icon": "images/deeplink-icon.png",
	"engines": {
		"vscode": "^1.29.0"
	},
	"categories": [
		"Other",
		"Snippets",
		"Debuggers"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"title": "[MobDevTools] Open deep link (Android ü§ñ)",
				"command": "extension.androidDeeplink"
			},
			{
				"title": "[MobDevTools] Open deep link (iOS üçé)",
				"command": "extension.iosDeeplink",
				"enablement": "isMac"
			},
			{
				"title": "[MobDevTools] Clear the deeplink history",
				"command": "extension.deeplinkClearCache"
			},
			{
				"title": "[MobDevTools] Open the latest used deeplink",
				"command": "extension.lastPromptDeeplink",
				"enablement": "false"
			},
			{
				"title": "[MobDevTools] Send push notification (iOS üçé)",
				"command": "extension.iosPushNotification",
				"enablement": "isMac"
			},
			{
				"title": "[MobDevTools] Send push notification (Android ü§ñ)",
				"command": "extension.androidPushNotification"
			},
			{
				"title": "[MobDevTools] Send the latest used notification",
				"command": "extension.pushNotificationLastUsed"
			},
			{
				"title": "[MobDevTools] Mirror device screen (Android ü§ñ)",
				"command": "extension.androidMirrorScreen"
			},
			{
				"title": "[MobDevTools] Start Device (Android ü§ñ)",
				"command": "extension.startAndroidDevice"
			},
			{
				"title": "[MobDevTools] Start Device (iOS üçé)",
				"command": "extension.startIosDevice",
				"enablement": "isMac"
			},
			{
				"title": "[MobDevTools] Connect Wifi (Android ü§ñ)",
				"command": "extension.connectWifi"
			},
			{
				"title": "[MobDevTools] Disconnect Wifi (Android ü§ñ)",
				"command": "extension.disconnectWifi"
			},
			{
				"title": "[MobDevTools] Reconnect offline devices (Android ü§ñ)",
				"command": "extension.reconnectOfflineWifi"
			},
			{
				"title": "[MobDevTools] Show active devices",
				"command": "extension.showConnectedDevices"
			},
			{
				"title": "[MobDevTools] Run Gist File",
				"command": "extension.runGistFile"
			},
			{
				"title": "[MobDevTools] Run Gist from User",
				"command": "extension.runGistFromUser"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^10.0.7",
		"@types/node": "^20.14.12",
		"@types/vscode": "^1.91.0",
		"tslint": "^6.1.3",
		"typescript": "^5.5.4"
	},
	"dependencies": {
		"@types/axios": "^0.14.0",
		"axios": "^1.7.2",
		"hasbin": "^1.2.3"
	},
	"activationEvents": [
		"onStartupFinished"
	]
}
